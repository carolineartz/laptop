#!/bin/sh

cd ~
curl --remote-name https://raw.githubusercontent.com/carolineartz/laptop/master/mac
curl --remote-name https://raw.githubusercontent.com/carolineartz/laptop/master/Brewfile
curl --remote-name https://raw.githubusercontent.com/carolineartz/laptop/master/Brewfile.ruby
/usr/bin/env bash mac 2>&1 | tee ~/laptop.log
